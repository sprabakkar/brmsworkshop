package redhat.brmsworkshop.lab1;

import com.redhat.brms.Driver;
import com.redhat.brms.Quote;

rule "DriverAgeEligibilityRules"
dialect "mvel"
salience 100
ruleflow-group "EligibilityRule"
    when
        $d : Driver( age < 18 )
    then
        Quote quote = new Quote();
	 quote.setEligibility( "NO" );
	 insertLogical( quote )
end